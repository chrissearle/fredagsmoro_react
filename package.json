    "chai-immutable": "^1.5.3",
    "codecov": "^1.0.1",
    "eslint": "^1.10.3",
    "imports-loader": "^0.6.5",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nyc": "^6.1.1",
    "react-addons-test-utils": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "bootstrap-loader": "^1.0.5",
