{
  "name": "fredagsmoro",
  "version": "0.0.3",
  "private": true,
  "description": "A collection of random images from the net - collected by Karl Ã˜gaard (previously Ragnar Bergvik) and sent out each Friday to amuse their colleagues.",
  "author": {
    "name": "Chris Searle",
    "web": "https://github.com/chrissearle"
  },
  "contributors": [
    {
      "name": "Rolf Erik Lekang",
      "url": "https://github.com/relekang"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:chrissearle/fredagsmoro_react.git"
  },
  "homepage": "https://fredagsmoro.chrissearle.org",
  "bugs": {
    "url" : "https://github.com/chrissearle/fredagsmoro_react/issues"
  },
  "scripts": {
    "lint": "eslint src test",
    "content:dropbox": "./scripts/dropbox.rb",
    "content:data": "./scripts/build_data.rb > ./build/site/data.json",
    "content:resize": "./scripts/resize.sh ./build/site/content",
    "docker:build": "./scripts/build_image.sh",
    "docker:deploy": "./scripts/deploy_image.sh",
    "doit": "./scripts/doit.sh",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js)'",
    "test:coverage": "nyc -a --require babel-core/register --require babel-polyfill npm run test",
    "test:watch": "npm run test -- --watch",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:codecov": "npm run coverage:report && codecov",
    "start": "webpack-dev-server --config webpack-dev.config.js"
  },
  "nyc": {
    "include": [
      "src/**"
    ]
  },
  "devDependencies": {
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "chai-immutable": "1.5.4",
    "codecov": "1.0.1",
    "eslint": "2.7.0",
    "eslint-plugin-mocha": "2.2.0",
    "eslint-plugin-react": "4.3.0",
    "imports-loader": "0.6.5",
    "jsdom": "8.3.1",
    "mocha": "2.4.5",
    "nyc": "6.4.0",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "sinon": "1.17.3",
    "webpack": "1.12.15",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "bootstrap-loader": "1.0.10",
    "bootstrap-sass": "3.3.6",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "ga-react-router": "2.0.0",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.1",
    "jquery": "2.2.3",
    "moment": "2.12.0",
    "node-sass": "3.4.2",
    "react": "15.0.1",
    "react-addons-pure-render-mixin": "15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.2.2",
    "react-router-redux": "4.0.2",
    "redux": "3.4.0",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7"
  }
